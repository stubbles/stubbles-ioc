{
    "name": "stubbles/ioc",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Dependency injection.",
    "require": {
        "php": "^8.2",
        "stubbles/sequence": "^10.1",
        "stubbles/values": "^11.0",
        "stubbles/reflect": "^10.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.11",
        "phpunit/phpunit": "^10.5",
        "bovigo/callmap": "^8.0",
        "bovigo/assert": "^8.0",
        "stubbles/streams": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\test\\": "src/test/helper",
            "stubbles\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
