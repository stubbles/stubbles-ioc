{
    "name": "stubbles/ioc",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Dependency injection.",
    "require": {
        "php": "^7.3",
        "stubbles/sequence": "^9.0",
        "stubbles/values": "^9.1",
        "stubbles/reflect": "^9.1"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.8",
        "phpunit/phpunit": "^9.0",
        "bovigo/callmap": "^6.1",
        "bovigo/assert": "^6.0",
        "stubbles/streams": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\test\\": "src/test/helper",
            "stubbles\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "php-coveralls": "php-coveralls"
    }
}
