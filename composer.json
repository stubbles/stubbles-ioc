{
    "name": "stubbles/ioc",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Dependency injection.",
    "require": {
        "php": "^7.0",
        "stubbles/sequence": "^8.1",
        "stubbles/values": "^8.0",
        "stubbles/reflect": "^8.0.1"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^5.4",
        "bovigo/callmap": "^3.0",
        "bovigo/assert": "^2.0",
        "stubbles/streams": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\test\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
