{
    "name": "stubbles/ioc",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Dependency injection.",
    "require": {
        "php": "^8.3",
        "stubbles/sequence": "^11.0",
        "stubbles/values": "^12.0",
        "stubbles/reflect": "^11.1",
        "stubbles/ioc-attributes": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.12",
        "phpunit/phpunit": "^12.4",
        "bovigo/callmap": "^9.0",
        "bovigo/assert": "^8.1",
        "stubbles/streams": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\test\\": "src/test/helper",
            "stubbles\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "12.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
